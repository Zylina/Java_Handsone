package day6Handsone;

public class NewEmployee {

	
		// TODO Auto-generated method stub
		int id,exYear;
		String name,skill;
		float salary;
		
	   public NewEmployee(int id, int exYear, String name, String skill,
				float salary) {
			super();
			this.id = id;
			this.exYear = exYear;
			this.name = name;
			this.skill = skill;
			this.salary = salary;
		}
	}


