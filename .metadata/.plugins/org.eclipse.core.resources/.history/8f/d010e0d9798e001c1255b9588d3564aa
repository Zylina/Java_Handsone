package day7Handsone;
import java.util.*; 
public class CompareExperience implements Comparator{

	public int compare(Object o1,Object o2){  
		d s1=(d)o1;  
		d s2=(d)o2;  
		  
		if(s1.experience==s2.experience)  
		return 0;  
		else if(s1.experience>s2.experience)  
		return 1;  
		else  
		return -1;  
		}  
       
	}


