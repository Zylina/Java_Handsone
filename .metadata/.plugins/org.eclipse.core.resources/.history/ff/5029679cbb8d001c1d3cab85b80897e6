package day6Handsone;

public class Employee {

	int id,exYear;
	String name,skill;
	float salary;
	
	public Employee(int id, int exYear, String name, String skill, float salary) {
		super();
		this.id = id;
		this.exYear = exYear;
		this.name = name;
		this.skill = skill;
		this.salary = salary;
	}

}
